	"cateManual": {
    "电影": "movie",
    "动画": "anime"

  },

    // 首页推荐视频的节点
	"homeVodNode": "//div[@class='item-thumbnail']/a",
    // 首页推荐视频的名称
	"homeVodName": "/@title",
    // 二次处理正则
    "homeVodNameR": "",
    // 首页推荐视频的id
	"homeVodId": "/@href",
    // 二次处理正则
	"homeVodIdR": "\\S+/(\\w+).html",
    // 首页推荐视频的图片
	"homeVodImg": "/img/@data-original",
    // 二次处理正则
    "homeVodImgR": "",
    // 首页推荐视频的简介
	"homeVodMark": "",
	"cateUrl": "https://www.novipnoad.com/{cateId}/page/{catePg}/",
    // 同上面的homeVod字段 分类列表中的视频信息
	"cateVodNode": "//div[@class='item-thumbnail']/a",
	"cateVodName": "",
	"cateVodId": "/@href",
	"cateVodIdR": "\\S+/(\\w+).html",
  	"cateVodImg": "/img/@data-original",
 	"cateVodMark": "",

    // 详情页地址 用于获取详情页信息 及 播放列表和地址
	"dtUrl": "https://www.subaibaiys.com/{cateId}/{vid}",
    // 详情节点
	"dtNode": "//div[@id='body']",
    // 视频名
	"dtName": "//[@class='video-item single-video-view post-134776 post type-post status-publish format-video has-post-thumbnail hentry category-anime tag-46326 post_format-post-format-video']/h1/text()",
	"dtNameR": "",
    // 视频图片
	"dtImg": "",
	"dtImgR": "",
    // 视频分类
	"dtCate": "//div[@class='item-tax-list']/strong[contains(text(),'Category: ')]/a/text()",
	"dtCateR": "",
    
    // 播放源节点
	"dtFromNode": "//div[contains('tm-multilink')]//div[@class='multilink-btn btn btn-sm btn-default bordercolor2hover bgcolor2hover ']",
    // 播放源名称
	"dtFromName": "/text()",
    // 播放列表节点
	"dtUrlNode": "//div[contains('tm-multilink')]//div[@class='multilink-btn btn btn-sm btn-default bordercolor2hover bgcolor2hover ']",
    // 播放地址节点
	"dtUrlSubNode": "/onclick",
    // 播放地址
	"dtUrlId": "@href",
    // 二次处理正则
	"dtUrlIdR": "/play/(\\S+).html",
    // 剧集名称
	"dtUrlName": "/text()",
    // 二次处理正则
	"dtUrlNameR": "",
    // 播放页面的地址 {playUrl} 对应上面 dtUrlId 获取到的地址
	"playUrl": "http://www.novipnoad.com/{cateId}/{playUrl}.html",
    // 解析webview的user-agent
	"playUa": "",
    // 搜索地址
	"searchUrl": "http://www.9rmb.com/search?wd={wd}",
    // 同上面的homeVod字段 搜索结果中的视频信息, 这里有对苹果cms suggest搜索接口的特殊支持，参考示例中的极品影视
	"scVodNode": "//div[@class='movie-item']/a",
	"scVodName": "/@title",
	"scVodId": "/@href",
	"scVodIdR": "/show/(\\w+).html",
	"scVodImg": "/img/@src",
	"scVodMark": "/button/text()"
