{
	"ua": "",
    // 首页地址 用于获取 分类和首页推荐
	"homeUrl": "https://www.novipnoad.com",
	"cateManual": {
    "电影": "movie",
    "动画": "anime"
  },

	"homeVodNode": "//div[contains(@id,'post-')]",
	"homeVodName": "//div[@class='item-head']/h3/a/text()",
	"homeVodId": "/@href",
	"homeVodIdR": "https://www.novipnoad.com/(\\w+).html",
	"homeVodImg": "//[@class='qv_tooltip tooltipstered']//a/img/@src",
    "homeVodImgR": "",
	"homeVodMark": "",
	"cateUrl": "https://www.novipnoad.com/{cateId}/page/{catePg}/",
	"cateVodNode": "//div[contains(@id,'post-')]",
	"cateVodName": "//div[@class='item-head']/h3/a/text()",
	"cateVodId": "/@href",
	"cateVodIdR": "https://www.novipnoad.com/(\\w+).html",
  	"cateVodImg": "//[@class='qv_tooltip tooltipstered']//a/img/@src",
 	"cateVodMark": "",

    // 详情页地址 用于获取详情页信息 及 播放列表和地址
	"dtUrl": "https://www.novipnoad.com/{vid}",
    // 详情节点
	"dtNode": "//head",
    // 视频名
	"dtName": "//meta[@property='og:title']/@content",
    //"dtName": "//body//h1[@class='light-title entry-title']/text()",
	"dtNameR": "",
    // 视频图片
	"dtImg": "//meta[@property='og:image']/@content",
	"dtImgR": "",
    // 视频分类
	"dtCate": "//div[@class='item-tax-list']/strong[contains(text(),'Category: ')]/a/text()",
	"dtCateR": "",
    
    // 播放源节点
	"dtFromNode": "//body//h1[@class='light-title entry-title']",
    // 播放源名称
	"dtFromName": "/text()",
    // 播放列表节点
	"dtUrlNode": "//head//meta[@property='og:url']",
    // 播放地址节点
	"dtUrlSubNode": "/onclick",
    // 播放地址
	"dtUrlId": "@content",
    // 二次处理正则
	"dtUrlIdR": "https://www.novipnoad.com/(\\S+).html",
    // 剧集名称
	"dtUrlName": "@content",
    // 二次处理正则
	"dtUrlNameR": "",
    // 播放页面的地址 {playUrl} 对应上面 dtUrlId 获取到的地址
	"playUrl": "http://www.novipnoad.com/{playUrl}.html",
    // 解析webview的user-agent
	"playUa": "",
    // 搜索地址
	"searchUrl": "http://www.9rmb.com/search?wd={wd}",
    // 同上面的homeVod字段 搜索结果中的视频信息, 这里有对苹果cms suggest搜索接口的特殊支持，参考示例中的极品影视
	"scVodNode": "//div[@class='movie-item']/a",
	"scVodName": "/@title",
	"scVodId": "/@href",
	"scVodIdR": "/show/(\\w+).html",
	"scVodImg": "/img/@src",
	"scVodMark": "/button/text()"
}